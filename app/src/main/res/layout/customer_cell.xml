<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <TextView
        android:id="@+id/cellPrenom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:maxLines="1"
        android:text="PrÃ©nom"
        android:textSize="25sp"
        android:translationX="120dp" />

    <TextView
        android:id="@+id/cellNom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="32dp"
        android:maxLines="1"
        android:text="Nom"
        android:textSize="25sp"
        android:translationX="120dp" />

    <ImageButton
        android:id="@+id/beforeButton"
        android:layout_width="63dp"
        android:layout_height="57dp"
        android:translationX="45dp"
        android:translationY="10dp"
        android:contentDescription="before button"
        app:srcCompat="@mipmap/ic_launcher_round" />

    <ImageButton
        android:id="@+id/afterButton"
        android:layout_width="63dp"
        android:layout_height="57dp"
        android:translationX="300dp"
        android:translationY="10dp"
        android:contentDescription="after button"
        app:srcCompat="@mipmap/ic_launcher" />


</RelativeLayout>